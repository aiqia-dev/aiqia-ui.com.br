{
  "$schema": "http://json.schemastore.org/cosmos-config",
  "vite": {
    "configPath": "vite.cosmos.config.js"
  },
  "plugins": [
    "react-cosmos-plugin-boolean-input",
    "react-cosmos-plugin-open-fixture",
    "react-cosmos-plugin-vite"
  ],
  "globalImports": [
    "lib/variables.css"
  ],
  "ignore": [
    "node_modules/**/*",
    "dist/**/*",
    "cosmos-export/**/*"
  ]
}